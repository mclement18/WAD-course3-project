<main class="mb-4">
  <div class="container">
    <h1 class="text-center p-3 mb-5 border border-top-0 border-left-0 border-right-0">Welcome to Pin Your Images App</h1>
    <h2 class="mb-4">Get inspiration</h2>
    <div class="container d-flex flex-wrap align-items-center">
      <% @pins.each do |pin| %>
        <%= render(partial: 'pin_card', locals: { pin: pin }) %>
      <% end %>
    </div>
  </div>
</main>