<%= form_tag(pins_path, method: :get, class: 'form-inline my-2 my-lg-0', role: 'search') do %>
  <%= search_field_tag('q', nil, class: 'form-control mr-sm-2', placeholder: 'Search') %>
  <%= button_tag('Search', class: 'btn btn-dark my-2 my-sm-0 mr-sm-2', name: nil) %>
  <%= link_to('See All', pins_path, class: 'btn btn-dark my-2 my-sm-0') %>
<% end %>